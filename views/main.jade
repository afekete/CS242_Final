body
   .site-wrapper
      .site-wrapper-inner
         .cover-container
            .masthead.clearfix
            .inner
               h3.masthead-brand Project
               ul.nav.masthead-nav
                  li
                     a(href='/') Home
                  li.active
                     a(href='/main') Main
            .page-header
               form(role='form')
                  .form-group
                     label(for='GivenTag') Tag
                     input.form-control(type='text', placeholder='Enter Tag')
                  button.btn.btn-default(type='submit') Submit

            .pattern#pattern
               ul.g
                  - for (var x = 0; x < 20; x++)
                     li
                        a(href='#')
                           img(src='http://bradfrost.github.com/this-is-responsive/patterns/images/fpo_square.png', alt='Product Name')

            .mastfoot
               .inner
                  P
                     a(href='/contact') Contact




